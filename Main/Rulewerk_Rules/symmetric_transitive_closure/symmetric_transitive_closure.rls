@source city[1]: load-csv("Rulewerk_Rules/symmetric_transitive_closure/sources/city.csv").
@source conn[2]: load-csv("Rulewerk_Rules/symmetric_transitive_closure/sources/conn.csv").

connected(?X,?Y) :- city(?X), city(?Y), conn(?X,?Y).
conn(?X,?Y) :- conn(?Y,?X).
connected(?X,?Y) :- city(?X), city(?XY), city(?Y), connected(?X,?XY), conn(?XY, ?Y).
